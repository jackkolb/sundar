# Humidity polling frequency (polls per second)
HUMIDITY_FREQUENCY=2

# Accelerometer polling frequency (polls per second)
ACCELEROMETER_FREQUENCY=203

# Current Sensor polling frequency (polls per second)
CURRENT_SENSOR_FREQUENCY=2
